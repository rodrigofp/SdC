<p id="notice"><%= notice %></p>

<h1>Prioridades</h1>

<table>
  <thead>
    <tr>
      <th>Nome</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @prioridades.each do |prioridade| %>
      <tr>
        <td><%= prioridade.nome %></td>
        <td><%= link_to 'Show', prioridade %></td>
        <td><%= link_to 'Edit', edit_prioridade_path(prioridade) %></td>
        <td><%= link_to 'Destroy', prioridade, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Prioridade', new_prioridade_path %>
