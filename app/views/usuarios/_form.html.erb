<%= form_with(model: usuario, local: true) do |form| %>
  <% if usuario.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(usuario.errors.count, "error") %> prohibited this usuario from being saved:</h2>

      <ul>
      <% usuario.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= form.label :nome %>
    <%= form.text_field :nome %>
  </div>

  <div class="field">
    <%= form.label :senha %>
    <%= form.password_field :senha %>
  </div>

  <div class="field">
    <%= form.label :cpf %>
    <%= form.text_field :cpf %>
  </div>

  <div class="field">
    <%= form.label :tipo_usuario_id %>
    <%= collection_select :usuario, :tipo_usuario_id, @tipo_usuarios_all, :id, :tipo %>
  </div>

  <div class="field">
    <%= form.label :cliente_id %>
    <%= collection_select :usuario, :cliente_id, @clientes_all, :id, :nome %>
  </div>

  <div class="actions">
    <%= form.submit %>
  </div>
<% end %>
