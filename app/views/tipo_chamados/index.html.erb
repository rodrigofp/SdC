<p id="notice"><%= notice %></p>

<h1>Tipo Chamados</h1>

<table>
  <thead>
    <tr>
      <th>Tipo</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tipo_chamados.each do |tipo_chamado| %>
      <tr>
        <td><%= tipo_chamado.tipo %></td>
        <td><%= link_to 'Show', tipo_chamado %></td>
        <td><%= link_to 'Edit', edit_tipo_chamado_path(tipo_chamado) %></td>
        <td><%= link_to 'Destroy', tipo_chamado, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tipo Chamado', new_tipo_chamado_path %>
