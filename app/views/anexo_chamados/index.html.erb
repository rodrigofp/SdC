<p id="notice"><%= notice %></p>

<h1>Anexo Chamados</h1>

<table>
  <thead>
    <tr>
      <th>Nome arquivo</th>
      <th>Atendimento chamado status</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @anexo_chamados.each do |anexo_chamado| %>
      <tr>
        <td><%= anexo_chamado.nome_arquivo %></td>
        <td><%= anexo_chamado.atendimento_chamado_status %></td>
        <td><%= link_to 'Show', anexo_chamado %></td>
        <td><%= link_to 'Edit', edit_anexo_chamado_path(anexo_chamado) %></td>
        <td><%= link_to 'Destroy', anexo_chamado, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Anexo Chamado', new_anexo_chamado_path %>
